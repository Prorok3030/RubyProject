<div id="<%= dom_id friend %>">
  <p>
    <strong>First name:</strong>
    <%= friend.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= friend.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= friend.email %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= friend.address %>
  </p>

  <p>
    <strong>Picture:</strong>
    <img src = '<%= friend.picture_url %>' width="130" height="110">
  </p>



</div>
